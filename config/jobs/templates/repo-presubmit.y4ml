presubmits:
  org/repo:
  - name: pr-something-unique
    always_run: true
    decorate: true
    branches:
    - master
    skip_report: false
    max_concurrency: 10
    spec:
      containers:
      - image: your container
        command:
        - /bin/bash
        args:
        - -c
        - "<A command>"